<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yswl.yswletc.dao.NewStudentMapper" >
  <resultMap id="BaseResultMap" type="com.yswl.yswletc.entity.NewStudent" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="name" property="name" jdbcType="VARCHAR" />
    <result column="age" property="age" jdbcType="INTEGER" />
    <result column="phone" property="phone" jdbcType="VARCHAR" />
    <result column="birthday" property="birthday" jdbcType="TIMESTAMP" />
  </resultMap>
  <delete id="deleteAll">
        delete from t_newstudent
    </delete>
  <insert id="insertAll" parameterType="java.util.List">
    INSERT INTO t_newstudent(id,name,age,phone,birthday)
    VALUES
    <foreach collection="list" item="item" index="index" separator="," >
      (#{item.id},#{item.name},#{item.age},#{item.phone},#{item.birthday})
    </foreach>
  </insert>
</mapper>